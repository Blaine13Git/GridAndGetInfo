<?xml version="1.0" encoding="UTF-8"?>
<suite name="nomalTest" parallel="false">
	<!-- 加入监听类 -->
	<listeners>
		<listener class-name="baseClass.MyTestngListener" />
		<listener class-name="baseClass.RetryListener" />
		<listener class-name="baseClass.TestListener" />

		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="smokeTest">
		<classes>

			<!-- 用户信息 -->
			<class name="smoke.b.qccr.AccoutInfo_NormalTest">
				<methods>
					<include name="storeAddress" />
					<include name="storeContect" />
					<include name="yingYeStartTime" />
					<include name="yingYeOverTime" />
					<include name="yingYeStatus" />
				</methods>
			</class>

						<!-- 采购订单各状态数量 -->
			<class name="smoke.b.qccr.PurchaseOrderNum_NormalTest">
				<methods>
					<include name="PurchaseOrderNum" />
				</methods>
			</class>
			
			
			<!-- 添加收货地址 -->
			<class name="smoke.b.qccr.AccoutInfoAddAddress_NormalTest">
				<methods>
					<include name="testAddDB" />
					<include name="testAddPage" />
				</methods>
			</class>

			<!-- 验证添加的收货地址 -->
			<class name="smoke.b.qccr.AccoutInfoReceiveGoodsAddressr_NormalTest">
				<methods>
					<include name="testAddPage" />
				</methods>
			</class>

			<!-- 购物车信息验证 -->
			<class name="smoke.b.qccr.ShoppingCart_NormalTest">
				<methods>
					<include name="testProductName" />
					<include name="testProductNum" />
					<include name="testProductPrice" />
				</methods>
			</class>

			<!-- 商品列表 -->
			<class name="smoke.b.qccr.GoodsList_NormalTest">
				<methods>
					<include name="testExist" />
					<include name="testProductName" />
				</methods>
			</class>

			<!-- 提款 -->
			<class name="smoke.b.qccr.Tikuan_NormalTest">
				<methods>
					<include name="testDrawings_amount" />
					<include name="testOrder_amount" />
					<include name="testOrder_numbers" />
					<include name="testReward_amount" />
				</methods>
			</class>
			

			<!-- 提款less -->
			<class name="smoke.b.qccr.TikuanLess_NormalTest" />

			<!-- 提款页面信息 -->
			<class name="smoke.b.qccr.TikuanMoney_NormalTest">
				<methods>
					<include name="testAbnormal_money" />
					<include name="testAudit_money" />
					<include name="testMoney_total" />
					<include name="testTotal_nomey" />
				</methods>
			</class>

			<!-- 异常订单信息 -->
			<class name="smoke.b.qccr.ExceptionOrders_NormalTest" />

			<!-- 验证码，正常用例 -->
			<class name="smoke.b.qccr.VerificationCode_NormalTest" />

			<!-- 验证码，异常用例 -->
			<class name="smoke.b.qccr.VerificationCode_AbormalTest" />

			<!-- 验证历史 全部 -->
			<class name="smoke.b.qccr.VerificationRecordsAll_NormalTest" />

			<!-- 验证历史 本月 -->
			<class name="smoke.b.qccr.VerificationRecordsMonth_NormalTest" />

			<!-- 验证历史 本周 -->
			<class name="smoke.b.qccr.VerificationRecordsWeek_NormalTest" />

			<!-- 验证历史 当天 -->
			<class name="smoke.b.qccr.VerificationRecordsToday_NormalTest" />
			

			<!-- <class name="smoke.b.qccr.AccoutInfo_NormalTest" /> -->

		</classes>
		<!-- <packages> <package name="qccr.b.somke" /> </packages> -->

	</test>
	<usedefaultlisteners name="false" />
</suite>